---
var defaultUrl = '/login'
---
<script src="/scripts/eruda.js" is:inline></script>
<script defaultUrl={defaultUrl}>
  var queryString = location.search
  var urlParams = new URLSearchParams(queryString)
  var page = urlParams.get('page') ? urlParams.get('page') : document.currentScript.getAttribute('defaultUrl')

  location.href = page
</script>